{
	"name": "Heroku cloud backup",
	"description": "Run cloud sync on heroku",
	"repository": "https://github.com/xenud/cloud-backup",
	"env": {
		"HEROKU_APP_NAME": {
			"description": "This should be the same as \"App name\" above."
		},
		"ARIA2C_SECRET": {
			"description": "Aria2c's secret",
			"value": "DEFAULT_SECRET"
		},
		"RCLONE_CONFIG_FILE": {
			"description": "Rclone config to mount in one line, replace line breaks with \"\\n\"",
			"required": true
		},
		"RCLONE_FIRST_COMMAND": {
			"description": "Command",
			"required": false
		},
		"RCLONE_SECOND_COMMAND": {
			"description": "Command",
			"required": false
		},
		"RCLONE_THIRD_COMMAND": {
			"description": "Command",
			"required": false
		},
		"RCLONE_FOURTH_COMMAND": {
			"description": "Command",
			"required": false
		},
		"RCLONE_FIFTH_COMMAND": {
			"description": "Command",
			"required": false
		}
	}
}
